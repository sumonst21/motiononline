{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "barryvdh/laravel-cors": "^0.11.2",
        "consoletvs/charts": "5.*",
        "cyrildewit/eloquent-viewable": "^3.2",
        "devmarketer/easynav": "^1.0",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "~3.3",
        "geoip2/geoip2": "^2.8",
        "intervention/image": "^2.4",
        "laravel-notification-channels/onesignal": "^1.2",
        "laravel/cashier": "~7.0",
        "laravel/framework": "5.5.*",
        "laravel/socialite": "3.0.9",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.5",
        "paypal/rest-api-sdk-php": "*",
        "pragmarx/tracker": "^3.2",
        "socialite-manager/gitlab-provider": "^1.0",
        "socialiteproviders/gitlab": "^3.1",
        "spatie/laravel-newsletter": "^4.2",
        "spatie/laravel-permission": "^2.31",
        "spatie/laravel-translatable": "^2.2",
        "stripe/stripe-php": "^5.8",
        "tzsk/payu": "^2.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
